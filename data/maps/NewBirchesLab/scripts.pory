mapscripts NewBirchesLab_MapScripts {
    MAP_SCRIPT_ON_TRANSITION: NewBirchesLab_EventScript_BallDefault
}
 
script NewBirchesLab_EventScript_BallDefault{
    setvar(VAR_STARTER_REGION, 0)
}
 
script NewBirchesLab_EventScript_Birch{
    lockall
    faceplayer
    if(!(flag(FLAG_HIDE_POKEBALL1) || flag(FLAG_HIDE_POKEBALL2) || flag(FLAG_HIDE_POKEBALL3))){
        msgbox("Hello, would you like to see more?", MSGBOX_YESNO)
        if(var(VAR_RESULT) == YES){
            if(var(VAR_STARTER_REGION) != 2){
                addvar(VAR_STARTER_REGION, 1)
            }
            else{
                setvar(VAR_STARTER_REGION, 0)
            }
            msgbox("Choose your Pokemon wisely")
        }
        else{
            msgbox("Choose your Pokemon wisely")
        }
    }
    else{
        msgbox("Take care of your Pokemon")
    }
    waitmessage
    releaseall
    end
}
 
script NewBirchesLab_EventScript_PokeballOne{
    lockall
    if(!(flag(FLAG_HIDE_POKEBALL1) || flag(FLAG_HIDE_POKEBALL2) || flag(FLAG_HIDE_POKEBALL3))){
        if(var(VAR_STARTER_REGION) == 0){
            call(NewBirchesLab_EventScript_Mudkip)
        }
        elif(var(VAR_STARTER_REGION) == 1){
            call(NewBirchesLab_EventScript_Totodile)
        }
        elif(var(VAR_STARTER_REGION) == 2){
            call(NewBirchesLab_EventScript_Squirtle)
        }
    }
    else{
        msgbox("Don't Take Another")
    }
    waitmessage
    releaseall
    end
}
 
